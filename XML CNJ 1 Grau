Elsif (assunto.cd_assunto_trf ='2567') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2569') then assunto.cd_assunto_pai := '1855';
Elsif (assunto.cd_assunto_trf ='2581') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2583') then assunto.cd_assunto_pai := '2594';
