Elsif (assunto.cd_assunto_trf ='1966') then assunto.cd_assunto_pai := '2656';
Elsif (assunto.cd_assunto_trf ='1976') then assunto.cd_assunto_pai := '2656';
Elsif (assunto.cd_assunto_trf ='1977') then assunto.cd_assunto_pai := '2656';
Elsif (assunto.cd_assunto_trf ='1978') then assunto.cd_assunto_pai := '2656';
