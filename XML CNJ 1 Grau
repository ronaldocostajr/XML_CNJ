Elsif (assunto.cd_assunto_trf ='2620') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2622') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2624') then assunto.cd_assunto_pai := '2622';
Elsif (assunto.cd_assunto_trf ='2641') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2656') then assunto.cd_assunto_pai := '2620';
Elsif (assunto.cd_assunto_trf ='2657') then assunto.cd_assunto_pai := '2656';
Elsif (assunto.cd_assunto_trf ='2661') then assunto.cd_assunto_pai := '2656';
