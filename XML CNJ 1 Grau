Elsif (assunto.cd_assunto_trf ='1998') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2019') then assunto.cd_assunto_pai := '2662';
Elsif (assunto.cd_assunto_trf ='2021') then assunto.cd_assunto_pai := '2662';
Elsif (assunto.cd_assunto_trf ='2029') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2031') then assunto.cd_assunto_pai := '2029';
Elsif (assunto.cd_assunto_trf ='2033') then assunto.cd_assunto_pai := '2029';
Elsif (assunto.cd_assunto_trf ='2037') then assunto.cd_assunto_pai := '2029';
Elsif (assunto.cd_assunto_trf ='2055') then assunto.cd_assunto_pai := '2458';
Elsif (assunto.cd_assunto_trf ='2086') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2116') then assunto.cd_assunto_pai := '2546';
