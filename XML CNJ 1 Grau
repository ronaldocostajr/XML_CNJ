Elsif (assunto.cd_assunto_trf ='2349') then assunto.cd_assunto_pai := '2458';
Elsif (assunto.cd_assunto_trf ='2364') then assunto.cd_assunto_pai := '2458';
Elsif (assunto.cd_assunto_trf ='2409') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2421') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2426') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2435') then assunto.cd_assunto_pai := '2620';
Elsif (assunto.cd_assunto_trf ='2445') then assunto.cd_assunto_pai := '2458';
Elsif (assunto.cd_assunto_trf ='2449') then assunto.cd_assunto_pai := '2546';
