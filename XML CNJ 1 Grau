Elsif (assunto.cd_assunto_trf ='2452') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2458') then assunto.cd_assunto_pai := '2458';
Elsif (assunto.cd_assunto_trf ='2461') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2463') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2466') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2468') then assunto.cd_assunto_pai := '2546';
