                    regexp_replace(cp.nr_cep, '[^0-9]', '', 'g') nr_cep,
                    trim(coalesce(cd_tipo_logradouro, '') || ' ' || coalesce(en.nm_logradouro, cp.nm_logradouro)) nm_logradouro, 
                    en.nr_endereco, 
