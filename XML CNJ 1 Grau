Elsif (assunto.cd_assunto_trf ='2233') then assunto.cd_assunto_pai := '2546';
Elsif (assunto.cd_assunto_trf ='2243') then assunto.cd_assunto_pai := '2620';
Elsif (assunto.cd_assunto_trf ='2266') then assunto.cd_assunto_pai := '5272';
Elsif (assunto.cd_assunto_trf ='2273') then assunto.cd_assunto_pai := '2458';
